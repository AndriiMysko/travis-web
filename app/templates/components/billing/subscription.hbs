<section>
  <section class="billing-subscription">
    <Billing::Summary
      @subscription={{this.subscription}}
      @v2subscription={{this.v2subscription}}
      @newSubscription={{this.newSubscription}}
      @account={{this.account}}
    />
  </section>
  {{#if (and this.subscription.isGithubResubscribable (not this.hasExpiredStripeSubscription))}}
    <Billing::Process
      @account={{this.account}}
      @newSubscription={{this.newSubscription}}
    />
  {{/if}}
</section>
